  "devDependencies": {
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.6",
    "@types/express-session": "^1.17.0",
    "@types/geojson": "^7946.0.7",
    "@types/keycloak-connect": "^4.5.4",
    "@types/morgan": "^1.9.1",
    "@types/node": "^14.0.14",
    "@types/sap__xsenv": "^2.0.1",
